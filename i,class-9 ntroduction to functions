def show_loading():
    for i in range(3):
        print("loading...")
a=5
b=7
print(a+b)
show_loading()
print(a-b)
show_loading()
print(a*b)
show_loading()
def sheldon_knock(name):
   for i in range(3):
       print("knock knock knock",name)
sheldon_knock("leonard")
def sheldon_knock(name,times=3)
       for i in range(times):
           print("knock knock knock",name)
sheldon_knock("penny",100)
